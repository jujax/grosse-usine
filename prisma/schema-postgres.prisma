generator client {
  provider = "prisma-client-js"
  output   = "./generated/postgres"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model AppUser {
  id      String      @id @db.Uuid
  email   String   @unique
  password String
  createdAt DateTime @default(now())
  admin   Boolean  @default(false)
  isEmailVerified Boolean @default(false)

}
